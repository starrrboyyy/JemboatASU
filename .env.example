# === RPC & Kontrak ===
RPC_URL=https://rpc.ankr.com/eth
CONTRACT_ADDRESS=0xYOUR_CONTRACT_ADDRESS
MINT_FUNC=mint
ABI_OVERRIDE=

# === Parameter Mint ===
MINT_PRICE=0.03
MINT_AMOUNT=1
GAS_LIMIT=300000
GAS_PRICE_GWEI=

# === Mode ===
# simple  -> snippet minimal (single wallet)
# single  -> advanced single wallet (retry, gas bump)
# multi   -> advanced multi-wallet
MODE=simple

PRIVATE_KEY=0xYOUR_PRIVATE_KEY                # untuk MODE=simple/single
PRIVATE_KEYS=0xKEY1,0xKEY2,0xKEY3             # untuk MODE=multi
TX_DELAY_MS=2500

# === Retry / Backoff ===
RETRY_ATTEMPTS=5
RETRY_BACKOFF_MS=2000
RETRY_BACKOFF_MULTIPLIER=1.6

# === Gas bump saat retry ===
GAS_BUMP_PERCENT=15

# === (Opsional) EIP-1559 override ===
MAX_FEE_GWEI=
MAX_PRIORITY_GWEI=
